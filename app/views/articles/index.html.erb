<div>
<%= link_to "Create an Article", new_article_path %>
</div>

<div>
  <% @articles.each do |art| %>
    <div>
      ID:<%= art.id%>
      Title: <%= art.title  %>
      <%= link_to "Edit",edit_article_path(art) %>
      <%= link_to "Show ",article_path(art) %>
      <%= link_to "Destroy",article_path(art), data: {
        turbo_method: :delete,
        turbo_confirm: "Are you sure?" }%>
    </div>
    <% end %>
</div>